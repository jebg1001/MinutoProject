 @model dynamic
  <br>

  <br>

  <br>

  <br>

  <br>

  <br>

  <div class="container"  style="text-align: center;">
    <div class="row">
      <div class="col-lg-12">
        <h2 style="color: black;">Enrutamiento</h2>
    <table class="table" >
      <thead>
        <tr style="background-color: #6fab2c;">
          <th style="color: white;" scope="col">Courier</th>
          <th style="color: white;" scope="col">Zona</th>
          <th style="color: white;" scope="col">Motivo</th>
      </thead>
      <tbody>
          @foreach (Enrutamiento en in @Model.Enrutamiento)
        {
          <tr>
            <th scope="row">@en.courier</th>
            <td>@en.zona</td>
            <td><ul><li>@en.motivo_1</li><li>@en.motivo_2</li><li>@en.motivo_3</li></ul></td>
          </tr>
        }
      </tbody>
    </table>
      </div>
    </div>
    <div class="row">
      <div style="text-align: center;">
        <button onclick="buscarRuta()" class="get-started-btn scrollto" style="border-radius: 50px;margin:20px; background-color: #6fab2c;">Generar Enrutamiento</button>
      </div>
    </div>
    <br>
    <br>
    <br>
    <br>
  </div>
  <script>
    function buscarRuta(){ setTimeout(() => {GetRuta()}, 10*1000); }
    function GetRuta(){window.location.href = "/Enrutamiento/GetRuta";}
  </script>
